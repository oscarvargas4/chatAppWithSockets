{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 13
  },
  "plugins": ["react"],
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "indent": "off",
    "consistent-return": "off",
    "comma-dangle": "off"
  }
}
